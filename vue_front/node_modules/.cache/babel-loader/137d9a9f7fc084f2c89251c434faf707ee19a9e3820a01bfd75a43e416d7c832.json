{"ast":null,"code":"export default {\n  data() {\n    return {\n      email: \"\",\n      nick: \"\",\n      image: \"\",\n      provider: \"\",\n      buttonList: [{\n        name: \"마이페이지\",\n        tab: \"myPage\"\n      }, {\n        name: \"내 코스\",\n        tab: \"myCourse\"\n      }, {\n        name: \"최근에 본 코스\",\n        tab: \"recentCourse\"\n      }, {\n        name: \"좋아요 리스트\",\n        tab: \"likeList\"\n      }, {\n        name: \"내가 쓴 댓글\",\n        tab: \"myComment\"\n      }, {\n        name: \"로그아웃\",\n        tab: \"logout\"\n      }],\n      isButtonsVisible: false,\n      activeTab: \"\"\n    };\n  },\n  mounted() {\n    this.email = localStorage.getItem(\"userID\"), this.nick = localStorage.getItem(\"userNick\"), this.image = localStorage.getItem(\"userImage\"), this.provider = localStorage.getItem(\"userProvider\");\n  },\n  methods: {\n    // 생략\n    toggleButtons() {\n      this.isButtonsVisible = !this.isButtonsVisible;\n    },\n    selectTab(tab) {\n      this.activeTab = tab;\n      if (tab === \"logout\") {\n        this.logout();\n      } else if (tab === \"myPage\") {\n        window.location.href = \"/mypage\";\n      } else if (tab === \"myCourse\") {\n        window.location.href = \"/mypage\";\n      }\n    },\n    logout() {\n      localStorage.removeItem(\"userID\");\n      localStorage.removeItem(\"userNick\");\n      localStorage.removeItem(\"userImage\");\n      localStorage.removeItem(\"userProvider\");\n      // localStorage.clear(); // localStorage의 모든 항목 제거\n      window.location.href = \"/\"; // 메인\n    }\n  }\n};","map":{"version":3,"names":["data","email","nick","image","provider","buttonList","name","tab","isButtonsVisible","activeTab","mounted","localStorage","getItem","methods","toggleButtons","selectTab","logout","window","location","href","removeItem"],"sources":["/Users/seoyeonju/Library/CloudStorage/OneDrive-계원예술대학교/풀스텍/weave-wego 2/vue_front/src/components/gnbBar.vue"],"sourcesContent":["<style src=\"../assets/css/gnbBar.css\"></style>\r\n<style src=\"../assets/css/reset.css\"></style>\r\n<template>\r\n  <div class=\"gnb\">\r\n    <div class=\"gnb_bar\">\r\n      <div class=\"gnb_bar_logo\">\r\n        <h2>\r\n          <a href=\"/\"><img src=\"../assets/img/logo.png\" alt=\"logo\" /></a>\r\n        </h2>\r\n      </div>\r\n      <div v-if=\"email === null\" class=\"gnb_bar_user\">\r\n        <p><a href=\"/login\">로그인</a></p>\r\n        <p><a href=\"/join\">회원가입</a></p>\r\n      </div>\r\n      <div v-else-if=\"provider === 'kakao'\" class=\"gnb_bar_user_login\">\r\n        <div\r\n          class=\"kakao_img\"\r\n          :style=\"{ 'background-image': 'url(' + image + ')' }\"\r\n          @click=\"toggleButtons\"\r\n        ></div>\r\n        <div class=\"gnbmypage\">\r\n          <div v-if=\"isButtonsVisible\">\r\n            <div v-for=\"button in buttonList\" :key=\"button.tab\">\r\n              <button\r\n                @click=\"selectTab(button.tab)\"\r\n                :class=\"{ 'active-button': activeTab === button.tab }\"\r\n              >\r\n                {{ button.name }}\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div v-else-if=\"provider === 'local'\" class=\"gnb_bar_user_login\">\r\n        <div\r\n          v-if=\"image\"\r\n          class=\"local_img\"\r\n          :style=\"{\r\n            'background-image': `url(http://localhost:3000/downloadProfile/${email}/${image})`,\r\n          }\"\r\n          @click=\"toggleButtons\"\r\n        ></div>\r\n\r\n        <div class=\"gnbmypage\">\r\n          <div v-if=\"isButtonsVisible\">\r\n            <div v-for=\"button in buttonList\" :key=\"button.tab\">\r\n              <button\r\n                @click=\"selectTab(button.tab)\"\r\n                :class=\"{ 'active-button': activeTab === button.tab }\"\r\n              >\r\n                {{ button.name }}\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div v-else-if=\"provider === 'naver'\" class=\"gnb_bar_user_login\">\r\n        <img\r\n          class=\"naver_img\"\r\n          :src=\"image\"\r\n          alt=\"profileExample\"\r\n          @click=\"toggleButtons\"\r\n        />\r\n        <div class=\"gnbmypage\">\r\n          <div v-if=\"isButtonsVisible\">\r\n            <div v-for=\"button in buttonList\" :key=\"button.tab\">\r\n              <button\r\n                @click=\"selectTab(button.tab)\"\r\n                :class=\"{ 'active-button': activeTab === button.tab }\"\r\n              >\r\n                {{ button.name }}\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      email: \"\",\r\n      nick: \"\",\r\n      image: \"\",\r\n      provider: \"\",\r\n      buttonList: [\r\n        { name: \"마이페이지\", tab: \"myPage\" },\r\n        { name: \"내 코스\", tab: \"myCourse\" },\r\n        { name: \"최근에 본 코스\", tab: \"recentCourse\" },\r\n        { name: \"좋아요 리스트\", tab: \"likeList\" },\r\n        { name: \"내가 쓴 댓글\", tab: \"myComment\" },\r\n        { name: \"로그아웃\", tab: \"logout\" },\r\n      ],\r\n      isButtonsVisible: false,\r\n      activeTab: \"\",\r\n    };\r\n  },\r\n  mounted() {\r\n    (this.email = localStorage.getItem(\"userID\")),\r\n      (this.nick = localStorage.getItem(\"userNick\")),\r\n      (this.image = localStorage.getItem(\"userImage\")),\r\n      (this.provider = localStorage.getItem(\"userProvider\"));\r\n  },\r\n  methods: {\r\n    // 생략\r\n    toggleButtons() {\r\n      this.isButtonsVisible = !this.isButtonsVisible;\r\n    },\r\n    selectTab(tab) {\r\n      this.activeTab = tab;\r\n      if (tab === \"logout\") {\r\n        this.logout();\r\n      } else if (tab === \"myPage\") {\r\n        window.location.href = \"/mypage\";\r\n      } else if (tab === \"myCourse\") {\r\n        window.location.href = \"/mypage\";\r\n      }\r\n    },\r\n    logout() {\r\n      localStorage.removeItem(\"userID\");\r\n      localStorage.removeItem(\"userNick\");\r\n      localStorage.removeItem(\"userImage\");\r\n      localStorage.removeItem(\"userProvider\");\r\n      // localStorage.clear(); // localStorage의 모든 항목 제거\r\n      window.location.href = \"/\"; // 메인\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAkFA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE,CACV;QAAEC,IAAI,EAAE,OAAO;QAAEC,GAAG,EAAE;MAAS,CAAC,EAChC;QAAED,IAAI,EAAE,MAAM;QAAEC,GAAG,EAAE;MAAW,CAAC,EACjC;QAAED,IAAI,EAAE,UAAU;QAAEC,GAAG,EAAE;MAAe,CAAC,EACzC;QAAED,IAAI,EAAE,SAAS;QAAEC,GAAG,EAAE;MAAW,CAAC,EACpC;QAAED,IAAI,EAAE,SAAS;QAAEC,GAAG,EAAE;MAAY,CAAC,EACrC;QAAED,IAAI,EAAE,MAAM;QAAEC,GAAG,EAAE;MAAS,CAAC,CAChC;MACDC,gBAAgB,EAAE,KAAK;MACvBC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACP,IAAI,CAACT,KAAI,GAAIU,YAAY,CAACC,OAAO,CAAC,QAAQ,CAAC,EACzC,IAAI,CAACV,IAAG,GAAIS,YAAY,CAACC,OAAO,CAAC,UAAU,CAAC,EAC5C,IAAI,CAACT,KAAI,GAAIQ,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC,EAC9C,IAAI,CAACR,QAAO,GAAIO,YAAY,CAACC,OAAO,CAAC,cAAc,CAAE;EAC1D,CAAC;EACDC,OAAO,EAAE;IACP;IACAC,aAAaA,CAAA,EAAG;MACd,IAAI,CAACN,gBAAe,GAAI,CAAC,IAAI,CAACA,gBAAgB;IAChD,CAAC;IACDO,SAASA,CAACR,GAAG,EAAE;MACb,IAAI,CAACE,SAAQ,GAAIF,GAAG;MACpB,IAAIA,GAAE,KAAM,QAAQ,EAAE;QACpB,IAAI,CAACS,MAAM,CAAC,CAAC;MACf,OAAO,IAAIT,GAAE,KAAM,QAAQ,EAAE;QAC3BU,MAAM,CAACC,QAAQ,CAACC,IAAG,GAAI,SAAS;MAClC,OAAO,IAAIZ,GAAE,KAAM,UAAU,EAAE;QAC7BU,MAAM,CAACC,QAAQ,CAACC,IAAG,GAAI,SAAS;MAClC;IACF,CAAC;IACDH,MAAMA,CAAA,EAAG;MACPL,YAAY,CAACS,UAAU,CAAC,QAAQ,CAAC;MACjCT,YAAY,CAACS,UAAU,CAAC,UAAU,CAAC;MACnCT,YAAY,CAACS,UAAU,CAAC,WAAW,CAAC;MACpCT,YAAY,CAACS,UAAU,CAAC,cAAc,CAAC;MACvC;MACAH,MAAM,CAACC,QAAQ,CAACC,IAAG,GAAI,GAAG,EAAE;IAC9B;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}