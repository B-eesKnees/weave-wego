{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  props: {\n    boardList: Object,\n    hideBrdOpen: Boolean,\n    editMode: Boolean\n  },\n  data() {\n    return {\n      selectedItems: [] // 선택한 항목의 BRD_ID 값을 담을 배열로 초기화\n    };\n  },\n\n  methods: {\n    updateSelectedItems(BRD_ID, checked) {\n      if (checked) {\n        this.selectedItems.push(BRD_ID);\n      } else {\n        const index = this.selectedItems.indexOf(BRD_ID);\n        if (index !== -1) {\n          this.selectedItems.splice(index, 1);\n          console.log(this.selectedItems.splice(index, 1));\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["props","boardList","Object","hideBrdOpen","Boolean","editMode","data","selectedItems","methods","updateSelectedItems","BRD_ID","checked","push","index","indexOf","splice","console","log"],"sources":["/Users/seoyeonju/Library/CloudStorage/OneDrive-계원예술대학교/풀스텍/weave-wego 2/vue_front/src/components/boardList.vue"],"sourcesContent":["<script>\nexport default {\n  props: {\n    boardList: Object,\n    hideBrdOpen: Boolean,\n    editMode: Boolean,\n  },\n  data() {\n    return {\n      selectedItems: [], // 선택한 항목의 BRD_ID 값을 담을 배열로 초기화\n    };\n  },\n  methods: {\n    updateSelectedItems(BRD_ID, checked) {\n      if (checked) {\n        this.selectedItems.push(BRD_ID);\n      } else {\n        const index = this.selectedItems.indexOf(BRD_ID);\n        if (index !== -1) {\n          this.selectedItems.splice(index, 1);\n          console.log(this.selectedItems.splice(index, 1));\n        }\n      }\n    },\n  },\n};\n</script>\n\n<template>\n  <div>\n    <a href=\"/detail\">\n      <input\n        class=\"mycourse_checkbox\"\n        type=\"checkbox\"\n        v-if=\"editMode\"\n        :checked=\"boardList.checked\"\n        @change=\"updateSelectedItems(boardList.BRD_ID, $event.target.checked)\"\n      />\n      <div class=\"board-list\">\n        <div class=\"board_content\">\n          <div class=\"hashtag\">{{ boardList.BRD_HASHTAG }}</div>\n          <div class=\"brd_created_at\">{{ boardList.BRD_CREATED_AT }}</div>\n          <div class=\"brd_title\">{{ boardList.BRD_TITLE }}</div>\n          <div class=\"viewer\">\n            <img src=\"../assets/img/view.png\" alt=\"view\" />\n            {{ boardList.BRD_VIEWCOUNT }}\n          </div>\n          <div class=\"like\">\n            <img src=\"../assets/img/like_on.png\" alt=\"like\" />\n            {{ boardList.likecount }}\n          </div>\n          <div v-if=\"!hideBrdOpen\" class=\"brd_open\">\n            {{ boardList.BRD_OPEN === 1 ? \"공개\" : \"비공개\" }}\n          </div>\n        </div>\n        <div\n          class=\"mypage_img\"\n          :style=\"`background-image: url(http://localhost:3000/downloadCourse/${boardList.BRD_ID}/${boardList.IMG_PATH})`\"\n        ></div>\n      </div>\n    </a>\n  </div>\n</template>\n"],"mappings":";AACA,eAAe;EACbA,KAAK,EAAE;IACLC,SAAS,EAAEC,MAAM;IACjBC,WAAW,EAAEC,OAAO;IACpBC,QAAQ,EAAED;EACZ,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE,EAAE,CAAE;IACrB,CAAC;EACH,CAAC;;EACDC,OAAO,EAAE;IACPC,mBAAmBA,CAACC,MAAM,EAAEC,OAAO,EAAE;MACnC,IAAIA,OAAO,EAAE;QACX,IAAI,CAACJ,aAAa,CAACK,IAAI,CAACF,MAAM,CAAC;MACjC,OAAO;QACL,MAAMG,KAAI,GAAI,IAAI,CAACN,aAAa,CAACO,OAAO,CAACJ,MAAM,CAAC;QAChD,IAAIG,KAAI,KAAM,CAAC,CAAC,EAAE;UAChB,IAAI,CAACN,aAAa,CAACQ,MAAM,CAACF,KAAK,EAAE,CAAC,CAAC;UACnCG,OAAO,CAACC,GAAG,CAAC,IAAI,CAACV,aAAa,CAACQ,MAAM,CAACF,KAAK,EAAE,CAAC,CAAC,CAAC;QAClD;MACF;IACF;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}